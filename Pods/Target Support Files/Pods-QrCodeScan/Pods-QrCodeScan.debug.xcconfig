GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CBImagePicker" "${PODS_ROOT}/Headers/Public/QrCodeScan"
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/CBImagePicker" "${PODS_CONFIGURATION_BUILD_DIR}/QrCodeScan"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CBImagePicker" -isystem "${PODS_ROOT}/Headers/Public/QrCodeScan"
OTHER_LDFLAGS = $(inherited) -ObjC -l"CBImagePicker" -l"QrCodeScan" -framework "AVFoundation"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
